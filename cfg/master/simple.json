    "ray": {
        "env_config": {
            "hab_cfg_path": "/root/vnav/cfg/pointnav.yaml"
        }, 
        "num_workers": 2,
        "num_gpus_per_worker": 1,
        "framework": "torch",
        "model": {
            "framestack": false
        }
