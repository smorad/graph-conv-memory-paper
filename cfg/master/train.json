{
    "ray": {
        "env_config": {
            "hab_cfg_path": "/root/vnav/cfg/objectnav_mp3d_train_val_mini.yaml",
            "preprocessors": {
                "compass": {
                    "module": "preprocessors.compass_fix",
                    "class": "CompassFix"
                },
                "semantic": {
                    "module": "preprocessors.sem_onehot",
                    "class": "SemanticOnehot"
                },
                "depth": {
                    "module": "preprocessors.quantized_depth",
                    "class": "QuantizedDepth"
                },
                "ghost_rgb": {
                    "module": "preprocessors.ghost_rgb",
                    "class": "GhostRGB"
                },
                "objectgoal": {
                    "module": "preprocessors.objectgoal",
                    "class": "ObjectGoalBoolean"
                }
            },
	    "rewards": {
                "stop_goal": {
		    "module": "rewards.basic",
	            "class":  "BasicReward"
	        },
		"goal_path": {
		    "module": "rewards.path",
		    "class": "PathReward"
		}
            }
        }, 
        "framework": "torch",
        "model": {
	    "framestack": false
	},
        "num_workers": 24,
        "num_gpus_per_worker": 0.12,
	"num_gpus": 1,
	"rollout_fragment_length": 256,
	"train_batch_size": 4096
    },
    "env_wrapper": {
        "module": "rayenv",
        "class": "NavEnv"
    },
    "trainer": {
       "module": "ray.rllib.agents.impala", 
       "class": "ImpalaTrainer"
    },
    "max_epochs": 100000
}
