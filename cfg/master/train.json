{
    "ray": {
        "env_config": {
            "hab_cfg_path": "/root/vnav/cfg/objectnav_mp3d_train_val_mini.yaml",
            "preprocessors": {
                "compass": {
                    "module": "preprocessors.compass_fix",
                    "class": "CompassFix"
                },
                "semantic": {
                    "module": "preprocessors.quantized_mash",
                    "class": "QuantizedSemanticMask"
                },
                "depth": {
                    "module": "preprocessors.quantized_depth",
                    "class": "QuantizedDepth"
                },
                "ghost_rgb": {
                    "module": "preprocessors.ghost_rgb",
                    "class": "GhostRGB"
                }
            },
	    "rewards": {
                "stop_goal": {
		    "module": "rewards.basic",
	            "class":  "BasicReward"
	        },
		"goal_path": {
		    "module": "rewards.path",
		    "class": "PathReward"
		}
            }
        }, 
        "framework": "torch",
        "model": {
	    "framestack": false
	},
        "num_workers": 20,
        "num_gpus_per_worker": 0.2,
	"num_gpus": 1,
	"rollout_fragment_length": 128,
	"train_batch_size": 2048,
	"lr": 0.0005,
	"num_sgd_iter": 3,
	"replay_proportion": 0.25,
	"replay_buffer_num_slots": 128
    },
    "env_wrapper": {
        "module": "rayenv",
        "class": "NavEnv"
    },
    "trainer": {
       "module": "ray.rllib.agents.impala", 
       "class": "ImpalaTrainer"
    },
    "callback": {
	"module": "custom_metrics",
	"class": "CustomMetrics"
    },
    "max_epochs": 100000
}
