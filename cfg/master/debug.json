{
    "ray": {
        "env_config": {
            "hab_cfg_path": "/root/vnav/cfg/objectnav_mp3d_debug.yaml",
            "preprocessors": {
                "compass": {
                    "module": "preprocessors.compass_fix",
                    "class": "CompassFix"
                },
                "semantic": {
                    "module": "preprocessors.mesh_semantic_downsample",
                    "class": "QuantizedSemanticMask"
                },
                "depth": {
                    "module": "preprocessors.quantized_depth",
                    "class": "QuantizedDepth"
                },
                "ghost_rgb": {
                    "module": "preprocessors.ghost_rgb",
                    "class": "GhostRGB"
                }
            },
	    "rewards": {
                "stop_goal": {
		    "module": "rewards.basic",
	            "class":  "BasicReward"
	        },
		"goal_path": {
		    "module": "rewards.path",
		    "class": "PathReward"
		}
            }
        }, 
        "num_workers": 0,
        "num_gpus_per_worker": 1,
        "framework": "torch",
        "model": {
            "framestack": false
        }
    },
    "env_wrapper": {
        "module": "rayenv",
        "class": "NavEnv"
    },
    "trainer": {
       "module": "ray.rllib.agents.ppo", 
       "class": "PPOTrainer"
    },
    "callback": {
	"module": "custom_metrics",
	"class": "CustomMetrics"
    },
    "max_epochs": 10
}
