{
    "ray": {
        "env_config": {
            "hab_cfg_path": "/root/vnav/cfg/objectnav_mp3d_human.yaml",
            "preprocessors": {
                "compass": {
                    "module": "preprocessors.compass_fix",
                    "class": "CompassFix"
                },
                "semantic": {
                    "module": "preprocessors.mesh_semantic_downsample",
                    "class": "QuantizedSemanticMask"
                },
                "depth": {
                    "module": "preprocessors.quantized_depth",
                    "class": "QuantizedDepth"
                },
                "ghost_rgb": {
                    "module": "preprocessors.ghost_rgb",
                    "class": "GhostRGB"
                },
		"vae": {
		    "module": "preprocessors.vae",
		    "class": "PPVAE"
		}
            },
	    "rewards": {
                "stop_goal": {
		    "module": "rewards.basic",
	            "class":  "BasicReward"
	        },
		"goal_path": {
		    "module": "rewards.path",
		    "class": "PathReward"
		}
            }
        } 
    },
    "env_wrapper": {
        "module": "graphenv",
        "class": "GraphNavEnv"
    },
    "callback": {
	"module": "custom_metrics",
	"class": "CustomMetrics"
    }
}
