{
    "ray": {
        "env_config": {
            "hab_cfg_path": "/root/vnav/cfg/objectnav_mp3d_train_autoencoder.yaml",
            "preprocessor": {
                "module": "preprocessors.autoencoder",
                "class": "EncoderPP"
            }
        }, 
        "num_workers": 0,
        "num_gpus_per_worker": 2,
        "framework": "torch",
        "model": {
            "framestack": false
        },
        "output": "/root/vnav/data/rgbd_320p"
    },
    "env_wrapper": {
        "module": "rayenv",
        "class": "NavEnv"
    },
    "trainer": {
       "module": "ray.rllib.agents.ppo", 
       "class": "PPOTrainer"
    },
    "max_epochs": 10
}
