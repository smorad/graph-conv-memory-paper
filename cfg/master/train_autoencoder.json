{
    "ray": {
        "env_config": {
            "hab_cfg_path": "/root/vnav/cfg/objectnav_mp3d_train_autoencoder.yaml",
            "preprocessors": {
                "semantic": {
                    "module": "preprocessors.sem_onehot",
                    "class": "SemanticOnehot"
                },
                "semantic": {
                    "module": "preprocessors.quantized_depth",
                    "class": "QuantizedDepth"
                },
                "ghost_rgb": {
                    "module": "preprocessors.ghost_rgb",
                    "class": "GhostRGB"
                }
            }
        }, 
        "num_workers": 4,
        "num_gpus_per_worker": 0.5,
        "framework": "torch",
        "model": {
	    "framestack": false
	}
    },
    "env_wrapper": {
        "module": "rayenv",
        "class": "NavEnv"
    },
    "trainer": {
       "module": "ray.rllib.agents.ppo", 
       "class": "PPOTrainer"
    },
    "max_epochs": 100000
}
