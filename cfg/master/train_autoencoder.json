{
    "ray": {
        "env_config": {
            "hab_cfg_path": "/root/vnav/cfg/objectnav_mp3d_train_autoencoder.yaml",
            "preprocessor": {
                "module": "preprocessors.noop",
                "class": "NoopPP"
            }
        }, 
        "num_workers": 4,
        "num_gpus_per_worker": 0.5,
        "framework": "torch",
	"train_batch_size": 64,
	"rollout_fragment_length": 64,
	"sgd_minibatch_size": 32,
        "model": {
	    "framestack": false,
	    "custom_model": "ray_vae"
	}
    },
    "env_wrapper": {
        "module": "rayenv",
        "class": "NavEnv"
    },
    "trainer": {
       "module": "ray.rllib.agents.ppo", 
       "class": "PPOTrainer"
    },
    "max_epochs": 100000
}
